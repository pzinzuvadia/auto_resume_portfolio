# Example secrets.toml file - DO NOT add real API keys here
# Create a copy of this file named "secrets.toml" with your actual API keys

# Anthropic API key for Claude
ANTHROPIC_API_KEY = "your_anthropic_api_key_here"

# PostgreSQL database connection (optional)
[postgres]
host = "your_db_host"
port = 5432
dbname = "your_db_name"
user = "your_db_user"
password = "your_db_password"